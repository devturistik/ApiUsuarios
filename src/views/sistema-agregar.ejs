<%- include('partials/header', { title: 'Dashboard', user: user }) %>

 <!-- Content wrapper -->
 <div class="content-wrapper">
    <!-- Content -->

    <div class="container-xxl flex-grow-1 container-p-y">
      <div class="app-ecommerce">
        <!-- Add Product -->
        <div
          class="d-flex flex-column flex-md-row justify-content-between align-items-start align-items-md-center mb-6 row-gap-4">
          <div class="d-flex flex-column justify-content-center">
            <h4 class="mb-1">Agregar Sistema</h4>
            <p class="mb-0">Sistema para los usuarios de los sistemas.</p>
          </div>
          <div class="d-flex align-content-center flex-wrap gap-4">
            <div class="d-flex gap-4">
              <button class="btn btn-label-secondary">Cancelar</button>
              <!-- <button class="btn btn-label-primary">Guardar</button> -->
            </div>
            <button type="submit" class="btn btn-primary">Guardar</button>
          </div>
        </div>

        <div class="row">
          <!-- First column-->
          <div class="col-12 col-lg-12">
            <!-- Product Information -->
            <div class="card mb-6">
              <div class="card-header">
                <h5 class="card-tile mb-0">Informacion del Sistema</h5>
              </div>
              <div class="card-body">
               
                <div class="row mb-6">
                    <div class="col">
                      <label class="form-label" for="ecommerce-product-sku">Nombre</label>
                      <input
                        type="number"
                        class="form-control"
                        id="ecommerce-product-sku"
                        placeholder="Nombre del Perfil"
                        name="perfilNombre"
                        aria-label="Product SKU" />
                    </div>
                    <div class="col">
                      <label class="form-label" for="ecommerce-product-barcode">Descripcion</label>
                      <input
                        type="text"
                        class="form-control"
                        id="ecommerce-product-barcode"
                        placeholder="Descripcion del perfil"
                        name="perfilDescripcion"
                        aria-label="Product barcode" />
                    </div>
                  </div>
               
              
              
              </div>
            </div>
           
            
          </div>
          <!-- /Second column -->

          <!-- Second column -->
       
          <!-- /Second column -->
        </div>
      </div>
    </div>


   <%- include('partials/footer-text')%>
    <!-- / Footer -->

    <div class="content-backdrop fade"></div>
  </div>
<%- include('partials/footer') %>
<script src="../../assets/js/app-turistik-sistema-add.js"></script>

</body>
</html>